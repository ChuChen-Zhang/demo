/**app.wxss**/
@import 'style/weui.wxss';
.container {
  font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
  font-size: 16px;
  display: flex;
  flex-direction: column;
  justify-content: space-between; 
  box-sizing: border-box;
  overflow-x: hidden;
  margin: 0 auto;
  padding: 0;
  width: 100%;
  height: 100%;
} 
page{
  background: #f8f8f8;
}
.container view{
  box-sizing: border-box;
}
